{
  "name": "vuex-toolkit",
  "description": "Vuex Toolkit. An alternative API for Vuex.",
  "version": "0.0.0",
  "scripts": {
    "clean": "rimraf lib && rimraf lib-types",
    "dev": "yarn clean && microbundle watch",
    "build": "yarn clean && microbundle",
    "release": "np",
    "prepack": "yarn build"
  },
  "engines": {
    "node": "12.18.1",
    "yarn": "1.22.4"
  },
  "dependencies": {
    "lodash": "^4.17.19"
  },
  "peerDependencies": {
    "redux-saga": "1.x.x",
    "vuex": "3.x.x"
  },
  "devDependencies": {
    "@redux-saga/types": "^1.1.0",
    "@types/lodash": "^4.14.151",
    "microbundle": "^0.12.3",
    "np": "^6.3.2",
    "redux": "4.x.x",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.7",
    "vue": "2.x.x",
    "vuex": "3.x.x"
  },
  "source": "src/index.ts",
  "main": "lib/vuex-toolkit.js",
  "module": "lib/vuex-toolkit.esm.js",
  "esmodule": "lib/vuex-toolkit.modern.js",
  "unpkg": "lib/vuex-toolkit.umd.js",
  "types": "lib-types/index.d.ts",
  "files": [
    "/lib",
    "/lib-types",
    "/src"
  ],
  "author": "Georgii (Egor) Gorbachev <hey.gorbachev@gmail.com>",
  "license": "MIT",
  "repository": "banderror/vuex-toolkit",
  "homepage": "https://github.com/banderror/vuex-toolkit",
