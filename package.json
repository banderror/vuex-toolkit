{
  "name": "vuex-toolkit",
  "description": "Vuex Toolkit. An alternative API for Vuex.",
  "version": "0.0.1",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "yarn clean && tsdx watch",
    "build": "yarn clean && tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "yarn build"
  },
  "dependencies": {
    "lodash": "^4.17.19",
    "tslib": "^2.0.0"
  },
  "peerDependencies": {
    "redux-saga": "1.x.x",
    "vuex": "3.x.x"
  },
  "devDependencies": {
    "@redux-saga/types": "^1.1.0",
    "@types/lodash": "^4.14.151",
    "husky": "^4.2.5",
    "redux": "4.x.x",
    "rimraf": "^3.0.2",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.7",
    "vue": "2.x.x",
    "vuex": "3.x.x"
  },
  "main": "dist/index.js",
  "module": "dist/vuex-toolkit.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "/dist",
    "/src"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "echo 'tsdx lint'"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Georgii (Egor) Gorbachev <hey.gorbachev@gmail.com>",
  "license": "MIT",
  "repository": "banderror/vuex-toolkit",
  "homepage": "https://github.com/banderror/vuex-toolkit",
  "bugs": "https://github.com/banderror/vuex-toolkit/issues",
  "keywords": [
    "vuex"
  ]
}
